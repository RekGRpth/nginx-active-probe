ngx_feature="ngx_http_upstream_conf_module"
ngx_feature_name=
ngx_feature_run=no
ngx_feature_incs=
ngx_feature_libs=""
ngx_feature_path="$ngx_addon_dir"
ngx_check_src="$ngx_addon_dir/src/ngx_http_upstream_conf_module.c \
             $ngx_addon_dir/src/ngx_http_upstream_conf_op.c     \
             $ngx_addon_dir/src/ngx_inet_slab.c               \
             "

ngx_feature_deps="$ngx_addon_dir/src/ngx_http_upstream_conf_module.h \
                $ngx_addon_dir/src/ngx_http_upstream_conf_op.h     \
                $ngx_addon_dir/src/ngx_inet_slab.h               \
                "
ngx_feature_test="int a;"
. auto/feature

if [ $ngx_found = yes ]; then
    have=NGX_HTTP_UPSTREAM_CONF . auto/have
    CORE_INCS="$CORE_INCS $ngx_feature_path"
    ngx_addon_name=ngx_http_upstream_conf_module
    ngx_module_type=HTTP
    ngx_module_name=$ngx_addon_name
    HTTP_MODULES="$HTTP_MODULES $ngx_addon_name"
    NGX_ADDON_DEPS="$NGX_ADDON_DEPS $ngx_feature_deps"
    NGX_ADDON_SRCS="$NGX_ADDON_SRCS $ngx_check_src"
else
    cat << END
    $0: error: the ngx_http_upstream_conf_module addon error.
END
    exit 1
fi
